  <artifactId>javahome-resolver-maven-plugin</artifactId>
  <version>1.0.2.0</version>
  <packaging>maven-plugin</packaging>
  <name>Maven java home resolver plugin</name>
  <description>This plugin resolves the java home path either configured via the toolchains plugin if used or via JAVA_HOME variable</description>
  <url>https://github.com/diamondq/javahome-resolver-maven-plugin/tree/master</url>
  <properties>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
    <maven.build.timestamp.format>dd.MM.yyyy HH:mm</maven.build.timestamp.format>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <nexus.serverId>ossrh</nexus.serverId>
    <nexus.url>https://oss.sonatype.org</nexus.url>
    <nexus.auto.release>true</nexus.auto.release>
    <nexus.skip.staging>false</nexus.skip.staging>
    <nexus.skip.local.staging>false</nexus.skip.local.staging>
    <scm.project>javahome-resolver-maven-plugin</scm.project>
  </properties>
  <licenses>
    <license>
      <name>Apache License, Version 2.0</name>
