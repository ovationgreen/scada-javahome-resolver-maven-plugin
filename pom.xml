        <role>committer</role>
      </roles>
    </developer>
    <developer>
      <name>Mike Mansell</name>
      <email>code@michaelmansell.com</email>
      <roles>
        <role>deployer</role>
      </roles>
    </developer>
  </developers>
  <scm>
